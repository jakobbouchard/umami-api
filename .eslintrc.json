{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:editorconfig/noconflict",
		"prettier"
	],
	"plugins": ["@typescript-eslint", "editorconfig"],
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"es2020": true,
		"node": true
	}
}
